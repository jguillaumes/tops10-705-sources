.DECLARE@BUILDS.DCL
.PA@BUILDS.PTH
.PA=BASE:
.DELETE BASE:MONGEN.AOK
.IF (ERROR)
.DELETE BASE:MONGEN.FUB
.IF (ERROR)
.LOAD/COMPIL/CREF MONGEN/SAVE
.IF (ERROR) .GOTO BADCOM
.CREF BASE:=MONGEN
.R FILCOM
*BASE:.SCM=MON:MONGEN.MAC,BASE:
